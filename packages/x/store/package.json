{
  "name": "@starbeamx/store",
  "version": "0.8.1",
  "description": "A Starbeam Store that's fun and easy to use",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    "default": "./index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "starbeam:type": "library:public",
  "scripts": {
    "test:lint": "eslint",
    "test:specs": "vitest --run",
    "test:types": "tsc -b"
  },
  "dependencies": {
    "@starbeam/debug": "workspace:^",
    "@starbeam/js": "workspace:^"
  },
  "devDependencies": {
    "@starbeam-dev/build-support": "workspace:*"
  }
}
