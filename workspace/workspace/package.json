{
  "private": true,
  "name": "@starbeam-workspace/workspace",
  "version": "1.0.0",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    "default": "./index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "starbeam": {
    "source": "ts",
    "type": "library:build-support"
  },
  "scripts": {
    "test:lint": "eslint \"index.ts\" \"src/**/*.ts\"",
    "test:types": "tsc -b"
  },
  "dependencies": {
    "@starbeam-workspace/paths": "workspace:^",
    "@starbeam-workspace/reporter": "workspace:^",
    "@types/shell-escape-tag": "workspace:^",
    "child_process": "^1.0.2",
    "shell-escape-tag": "^2.0.2"
  },
  "devDependencies": {
    "@starbeam-dev/build-support": "workspace:*"
  }
}
