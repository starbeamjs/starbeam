{
  "private": true,
  "name": "@starbeam-demos/preact-hello-world",
  "version": "1.0.0",
  "type": "module",
  "main": "index.ts",
  "exports": {
    "default": "./index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "starbeam": {
    "jsx": "preact",
    "source": "tsx",
    "type": "demo:preact",
    "used": [
      {
        "packages": [
          "purecss"
        ],
        "reason": "css"
      }
    ]
  },
  "scripts": {
    "test:lint": "eslint \"src/**/*.{tsx,ts}\"",
    "test:types": "tsc -b"
  },
  "dependencies": {
    "@starbeam/collections": "workspace:^",
    "@starbeam/preact": "workspace:^",
    "@starbeam/runtime": "workspace:^",
    "@starbeam/universal": "workspace:^",
    "preact": "*",
    "purecss": "3.0.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@vitest/ui": "^0.30.1",
    "vite": "*",
    "vitest": "^0.30.1"
  }
}
