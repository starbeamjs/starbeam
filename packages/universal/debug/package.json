{
  "name": "@starbeam/debug",
  "version": "0.8.9",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    "default": "./index.ts"
  },
  "publishConfig": {
    "exports": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "starbeam": {
    "inline": [
      "stacktracey",
      "get-source",
      "source-map",
      "data-uri-to-buffer",
      "as-table",
      "printable-characters",
      "chalk"
    ],
    "type": "library:public"
  },
  "scripts": {
    "test:lint": "eslint . --max-warnings 0",
    "test:specs": "vitest --run",
    "test:types": "tsc -b"
  },
  "dependencies": {
    "@starbeam/core-utils": "workspace:^",
    "@starbeam/interfaces": "workspace:^",
    "@starbeam/reactive": "workspace:^",
    "@starbeam/shared": "workspace:^",
    "@starbeam/tags": "workspace:^",
    "@starbeam/verify": "workspace:^",
    "buffer": "^6.0.3",
    "stacktracey": "^2.1.8"
  },
  "devDependencies": {
    "@starbeam-dev/compile": "workspace:^",
    "@starbeam-dev/eslint-plugin": "^1.0.4",
    "@types/node": "18.16.1",
    "rollup": "^4.0.2"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.md"
  ]
}
