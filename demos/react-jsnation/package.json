{
  "private": true,
  "name": "@starbeam-demos/react-jsnation",
  "version": "1.0.0",
  "type": "module",
  "main": "index.ts",
  "exports": {
    "default": "./index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "starbeam": {
    "jsx": "react",
    "source": "tsx",
    "type": "demo:react",
    "used": [
      {
        "packages": [
          "purecss"
        ],
        "reason": "css"
      }
    ]
  },
  "scripts": {
    "test:lint": "eslint \"src/**/*.{tsx,ts}\"",
    "test:specs": "vitest --run",
    "test:types": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@starbeam/collections": "workspace:^",
    "@starbeam/debug": "workspace:^",
    "@starbeam/interfaces": "workspace:^",
    "@starbeam/react": "workspace:^",
    "@starbeam/reactive": "workspace:^",
    "@starbeam/resource": "workspace:^",
    "@starbeam/universal": "workspace:^",
    "purecss": "3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@vitest/ui": "*",
    "vite": "4.3.3"
  }
}
