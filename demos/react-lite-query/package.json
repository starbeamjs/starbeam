{
  "private": true,
  "name": "@starbeam-demos/react-lite-query",
  "version": "1.0.0",
  "type": "module",
  "main": "index.ts",
  "exports": {
    "default": "./index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "starbeam": {
    "source": "tsx",
    "type": "demo:react"
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "test:lint": "eslint \"src/**/*.{tsx,ts}\" --max-warnings 0",
    "test:types": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@starbeam/collections": "workspace:^",
    "@starbeam/debug": "workspace:^",
    "@starbeam/interfaces": "workspace:^",
    "@starbeam/react": "workspace:^",
    "@starbeam/runtime": "workspace:^",
    "@starbeam/universal": "workspace:^",
    "@starbeam/verify": "workspace:^",
    "axios": "^1.3.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@starbeam/eslint-plugin": "workspace:^",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "vite": "4.3.9",
    "vitest": "^0.32.2"
  }
}
