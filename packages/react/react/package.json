{
  "name": "@starbeam/react",
  "version": "0.8.3",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    "default": "./index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "starbeam": {
    "source": "tsx",
    "type": "library:public"
  },
  "scripts": {
    "test:lint": "eslint",
    "test:specs": "vitest --run",
    "test:types": "tsc -b"
  },
  "dependencies": {
    "@starbeam/core-utils": "workspace:^",
    "@starbeam/debug": "workspace:^",
    "@starbeam/js": "workspace:^",
    "@starbeam/modifier": "workspace:^",
    "@starbeam/shared": "workspace:^",
    "@starbeam/timeline": "workspace:^",
    "@starbeam/universal": "workspace:^",
    "@starbeam/use-strict-lifecycle": "workspace:^",
    "@starbeam/verify": "workspace:^"
  },
  "devDependencies": {
    "@domtree/flavors": "workspace:^",
    "@starbeam-dev/build-support": "workspace:*",
    "@types/react": "^18.0.9"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
