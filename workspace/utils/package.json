{
  "private": true,
  "name": "@starbeam-workspace/utils",
  "version": "1.0.0",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    "default": "./index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "starbeam": {
    "source": "ts",
    "type": "library:build-support"
  },
  "scripts": {
    "test:lint": "eslint . --max-warnings 0",
    "test:types": "tsc -b"
  },
  "dependencies": {
    "@starbeam-workspace/json": "workspace:^",
    "@starbeam-workspace/reporter": "workspace:^",
    "@starbeam-workspace/shared": "workspace:^"
  },
  "devDependencies": {
    "@starbeam/eslint-plugin": "workspace:^",
    "@starbeam-dev/build-support": "workspace:^",
    "@types/shell-escape-tag": "workspace:^"
  }
}
