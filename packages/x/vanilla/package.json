{
  "name": "@starbeamx/vanilla",
  "version": "0.8.9",
  "description": "A renderer for Starbeam using Vanilla JavaScript",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    "default": "./index.ts"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.cjs"
      }
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "starbeam:type": "library:public",
  "scripts": {
    "test:lint": "eslint \"index.ts\" \"src/**/*.ts\"",
    "test:specs": "vitest --run",
    "test:types": "tsc -b"
  },
  "dependencies": {
    "@starbeam/debug": "workspace:^",
    "@starbeam/interfaces": "workspace:^",
    "@starbeam/reactive": "workspace:^",
    "@starbeam/runtime": "workspace:^",
    "@starbeam/universal": "workspace:^"
  },
  "devDependencies": {
    "@starbeam-dev/build-support": "workspace:*",
    "rollup": "^3.20.6",
    "typescript": "^5.0.4"
  }
}
