{
  "name": "@starbeam/shared",
  "version": "1.3.7",
  "description": "A package that facilitates having multiple copies of Starbeam in a single process that interoperate with each other",
  "type": "module",
  "main": "index.ts",
  "types": "index.ts",
  "exports": {
    "default": "./index.ts"
  },
  "publishConfig": {
    "exports": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "main": "dist/index.cjs",
    "types": "dist/index.d.ts"
  },
  "starbeam": {
    "type": "library:public"
  },
  "scripts": {
    "test:lint": "eslint . --max-warnings 0",
    "test:specs": "vitest --run",
    "test:types": "tsc -b"
  },
  "devDependencies": {
    "@starbeam/eslint-plugin": "workspace:^",
    "@starbeam-dev/build-support": "workspace:^",
    "rollup": "^3.29.2"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE.md"
  ]
}
